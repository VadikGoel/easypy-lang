use gui

# UGLY FORMATTING TEST
# This tests if the normalizer handles messy one-liners
class MessyApp { func run(){ print("Messy App Running!"); if(1==1){print("Logic Works")} } }

app = gui.create_app("Messy Formatting Test", 300, 200)

# Multi-statement on one line
gui.label(app, "It works!"); print("Console log check")

gui.button(app, "Click me", MessyApp().run)

gui.show(app)
